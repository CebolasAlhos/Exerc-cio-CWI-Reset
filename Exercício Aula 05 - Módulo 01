exercicio 5
receitaLiquida = 1000
custoDosProdutos = 500
lucroBruto = (receitaLiquida - custoDosProdutos)
margembruta = (lucroBruto / receitaLiquida) * 100

exercicio 6
saldoDaConta = 1000
saldoDaConta = saldoDaConta - 99.90
saldoDaConta = saldoDaConta + 2500
saldoDaConta = saldoDaConta - 0.1
saldoDaConta = saldoDaConta * 0.75
saldoDaConta = saldoDaConta / 2
